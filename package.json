{
  "name": "focus-space",
  "displayName": "Focus Space",
  "description": "Maintain a focused view of files and folders for large projects",
  "version": "0.0.1",
  "publisher": "your-publisher",
  "engines": {
    "vscode": "^1.93.0"
  },
  "categories": [
    "Other"
  ],
  "keywords": [
    "focus",
    "productivity",
    "files",
    "workspace",
    "copilot"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "focusSpace.test",
        "title": "Test Focus Space"
      },
      {
        "command": "focusSpace.testData",
        "title": "Test Focus Space Data"
      },
      {
        "command": "focusSpace.populateTestData",
        "title": "Populate Focus Space Test Data"
      }
    ],
    "views": {
      "explorer": [
        {
          "id": "focusSpace",
          "name": "Focus Space",
          "icon": "$(target)",
          "contextualTitle": "Focus Space",
          "when": "focusSpace.hasItems"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.93.0",
    "@types/node": "20.x",
    "@types/mocha": "^10.0.1",
    "@typescript-eslint/eslint-plugin": "^6.4.1",
    "@typescript-eslint/parser": "^6.4.1",
    "@vscode/test-cli": "^0.0.4",
    "@vscode/test-electron": "^2.3.4",
    "eslint": "^8.47.0",
    "mocha": "^10.2.0",
    "typescript": "^5.1.6"
  }
}